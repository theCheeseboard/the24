<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.vicr123.the24.Alarm">
    <signal name="Removing">
    </signal>
    <signal name="ActiveChanged">
      <arg name="active" type="b" direction="out"/>
    </signal>
    <signal name="OffsetChanged">
      <arg name="offset" type="t" direction="out"/>
    </signal>
    <signal name="SnoozeOffsetChanged">
      <arg name="offset" type="x" direction="out"/>
    </signal>
    <signal name="RepeatChanged">
      <arg name="repeat" type="t" direction="out"/>
    </signal>
    <method name="Remove">
    </method>
    <method name="Active">
      <arg type="b" direction="out"/>
    </method>
    <method name="Offset">
      <arg type="t" direction="out"/>
    </method>
    <method name="Repeat">
      <arg type="t" direction="out"/>
    </method>
    <method name="SnoozeOffset">
      <arg type="x" direction="out"/>
    </method>
    <method name="SetActive">
      <arg name="active" type="b" direction="in"/>
    </method>
    <method name="SetOffset">
      <arg name="offset" type="t" direction="in"/>
    </method>
    <method name="SetRepeat">
      <arg name="repeat" type="t" direction="in"/>
    </method>
  </interface>
</node>
